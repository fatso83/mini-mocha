{
  "name": "@fatso83/mini-mocha",
  "version": "2.1.0",
  "description": "A minimal emulation of Mocha",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "fixup": "prettier --write *.js",
    "test": "node test/tests.js",
    "update-snapshots": "rm -r test/snapshot/* && npm t"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fatso83/mini-mocha.git"
  },
  "keywords": [
    "mocha",
    "testing",
    "demo"
  ],
  "author": "Carl-Erik Kopseng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fatso83/mini-mocha/issues"
  },
  "devDependencies": {
    "husky": "^2.3.0",
    "prettier": "^1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier -c *.js && node test/tests.js"
    }
  }
}
